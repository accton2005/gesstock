{
    "name": "admin/ges-stock",
    "description": "Application de gestion de magasin pour administration publique",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0",
        "laravel/tinker": "^2.7",
        "barryvdh/laravel-dompdf": "^1.0",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
